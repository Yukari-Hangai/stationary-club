<h1>新規投稿</h1>

  <%= form_with model: @post do |f| %>
    <table class="table">
      <tr>
        <th>投稿画像</th>
        <th><%= f.file_field :image, accept: "image/*" %></th>
      </tr>
      <tr>
        <th>本文</th>
        <th><%= f.text_area :text %></th>
      </tr>
    </table>
    <%= f.submit '投稿' %>
  <% end %>
